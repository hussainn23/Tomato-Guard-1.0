<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:exitFadeDuration="100">
    <item android:state_selected="true" android:state_pressed="true" android:state_focused="true" android:state_checked="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="?colorPrimary"/>
            <corners android:radius="35dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="?colorButtonNormal"/>
            <corners android:radius="35dp"/>
        </shape>
    </item>

</selector>